{
	"neo4j": {
		"uri": "bolt://localhost:7687",
		"username": "neo4j",
		"password": "1234",
		"db": null,
		"file_folder": "/import/",
		"insertion_mode": "csv_file",
		"clear_data": true,
		"cap_insert": 25000
	},
	"csv_map":{
		"directors": {
			"filepath_or_buffer": "src/files/archive/900_acclaimed_directors_awards.csv",
			"sep": ";"
		},
		"movies": {
			"filepath_or_buffer": "src/files/archive/AllMoviesDetailsCleaned.csv",
			"sep": ";"
		},
		"cast": {
			"filepath_or_buffer": "src/files/archive/AllMoviesCastingRaw.csv",
			"sep": ";"
		}
	},
	"etl":{
		"directors": {
			"node_name": "Person",
			"column_map": {
				"name": "name"
			}
		},
		"movies": {
			"node_name": "Movie",
			"column_map": {
				"id": "movie_id",
				"imdb_id": "imdb_id",
				"genres": "genres",
				"original_language": "lang",
				"original_title": "title",
				"overview": "overview"
			}
		},
		"cast": {
			"node_name": "Person",
			"column_map": {
				"actor1_name": "actor_1",
				"actor1_gender": "actor_1_gender",
				"actor2_name": "actor_2",
				"actor2_gender": "actor_2_gender",
				"actor3_name": "actor_3",
				"actor3_gender": "actor_3_gender",
				"actor4_name": "actor_4",
				"actor4_gender": "actor_4_gender",
				"actor5_name": "actor_5",
				"actor5_gender": "actor_5_gender"
			}
		}
	}
}